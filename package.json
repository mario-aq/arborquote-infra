{
  "name": "arborquote-infra",
  "version": "0.1.0",
  "description": "AWS CDK infrastructure for ArborQuote MVP backend",
  "bin": {
    "arborquote-infra": "dist/bin/arborquote-infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:e2e": "./scripts/test-e2e.sh",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --profile arborquote",
    "diff": "cdk diff",
    "local:start": "docker-compose up -d",
    "local:stop": "docker-compose down",
    "local:logs": "docker-compose logs -f localstack",
    "local:deploy": "cdklocal deploy --require-approval never",
    "local:destroy": "cdklocal destroy",
    "local:test": "./scripts/test-local.sh"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.940.0",
    "@aws-sdk/client-s3": "^3.940.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "aws-cdk": "2.1033.0",
    "aws-sdk": "^2.1692.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.230.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
